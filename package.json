{
  "name": "@pentops/jsonapi-jdef-ts-generator",
  "version": "0.0.98",
  "description": "Generate types from a jdef.json or api.json file served by your pentops j5 registry.",
  "main": "dist/index.js",
  "bin": {
    "jdef-ts-generator": "./bin/index.js"
  },
  "type": "module",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "start": "pnpm run build && node bin/index.js",
    "test": "vitest run",
    "clean": "rm -rf dist",
    "build": "pnpm run clean && rollup -c rollup.config.js",
    "publish": "pnpm run build && pnpm publish",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "keywords": [],
  "author": "Joshua Slate",
  "license": "MIT",
  "dependencies": {
    "change-case": "^5.4.4",
    "find-up": "^7.0.0",
    "mitt": "^3.0.1",
    "pretty-ms": "^9.1.0",
    "ts-morph": "^23.0.0",
    "typescript": "^5.6.2"
  },
  "devDependencies": {
    "@pentops/jsonapi-request": "^0.0.6",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.0",
    "@types/node": "^22.7.4",
    "prettier": "^3.3.3",
    "rollup": "^4.22.5",
    "ts-pattern": "^5.4.0",
    "tslib": "^2.7.0",
    "vitest": "^2.1.1"
  },
  "peerDependencies": {
    "@pentops/jsonapi-request": ">=0.0.4"
  }
}
