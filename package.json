{
  "name": "@pentops/jsonapi-jdef-ts-generator",
  "version": "0.0.69",
  "description": "Generate types from a jdef.json or api.json file served by your pentops j5 registry.",
  "main": "dist/index.js",
  "bin": {
    "jdef-ts-generator": "./bin/index.js"
  },
  "type": "module",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "start": "pnpm run build && node bin/index.js",
    "test": "vitest run",
    "clean": "rm -rf dist",
    "build": "pnpm run clean && rollup -c rollup.config.js",
    "publish": "pnpm run build && pnpm publish",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "keywords": [],
  "author": "Joshua Slate",
  "license": "MIT",
  "dependencies": {
    "change-case": "^5.4.4",
    "find-up": "^7.0.0",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@pentops/jsonapi-request": "^0.0.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^22.5.0",
    "prettier": "^3.3.3",
    "rollup": "^4.21.0",
    "ts-pattern": "^5.3.1",
    "tslib": "^2.6.3",
    "vitest": "^2.0.5"
  },
  "peerDependencies": {
    "@pentops/jsonapi-request": ">=0.0.4"
  }
}
